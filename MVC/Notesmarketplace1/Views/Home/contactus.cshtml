@model Notesmarketplace1.Models.Contactmodel
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1.0 ,user-scalable=no">
    <!--title-->
    <title>Notes Marketplace</title>
    <!--Favicon-->
    <!--Google fonts-->
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;400;600;700&display=swap" rel="stylesheet">
    <!--Font awesome-->
    <link href="~/Content/css/fontawesome/css/font-awesome.min.css" rel="stylesheet" />
    @*<link rel="stylesheet" href="css/fontawesome/css/font-awesome.min.css">*@
    <link href="~/Content/css/responsive-tabs/responsive-tabs.min.css" rel="stylesheet" />
    @* <link rel="stylesheet" href="css/responsive-tabs/responsive-tabs.min.css">*@
    <!--Bootstrap CSS-->
    <link href="~/Content/css/bootstrap/bootstrap.min.css" rel="stylesheet" />
    @* <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css">*@

    <!--Custom CSS-->

    <link href="~/Content/css/style.css" rel="stylesheet" />

    @* <link rel="stylesheet" href="css/style.css">*@
    <link href="~/Content/css/responsive-tabs/responsive.css" rel="stylesheet" />
    @* <link rel="stylesheet" href="css/responsive-tabs/responsive.css">*@

</head>

<body>

    <!--user profile page-->


    <header>
        <nav class="navbar">
            <div class="container-fluid">
                <div class="site-nav-wrapper">
                    <div class="navbar-header ">
                        <!--mobile menu open button-->
                        <span id="mobile-nav-open-btn">&#9776;</span>

                        <!--logo-->
                        <a href="#" class="navbar-brand smooth-scroll">

                            <img src="~/Content/img/pre-login/purple-logo.png" / class="img-responsive">
                            @* <img src="img/pre-login/purple-logo.png" alt="logo" class="img-responsive">*@
                        </a>
                    </div>
                    <!--main menu-->
                    <div class="container01">
                        <div class="collapse navbar-collapse">
                            <ul class="nav navbar-nav pull-right">
                                <li><a href="/Notes/Searchnote">Search Notes</a></li>
                                <li><a href="/Notes/Dashboard">Sell Your Notes</a></li>

                                <li><a href="/Home/FAQ">FAQ</a></li>
                                <li><a href="/Home/Contactus">Contact Us</a></li>

                                <li><a href="#"><button class="btn btn-primary" type="submit" id="btn-logout">Login</button></a></li>

                            </ul>
                        </div>
                    </div>
                    <!--mobile menu-->
                    <div id="mobile-nav">
                        <!--mobile menu open button-->
                        <span id="mobile-nav-close-btn">&times;</span>
                        <div id="mobile-nav-content">
                            <ul class="nav">

                                <li><a class="smooth-scroll" href="#home">Search Notes</a></li>
                                <li><a class="smooth-scroll" href="#about">Sell Your Notes</a></li>

                                <li><a class="smooth-scroll" href="#services">FAQ</a></li>
                                <li><a class="smooth-scroll" href="#portfolio">Contact Us</a></li>

                                <li><a class="smooth-scroll" href="#contact"><button class="btn btn-primary" type="submit" id="btn-logout">Logout</button></a></li>
                            </ul>

                        </div>
                    </div>
                </div>
            </div>

        </nav>
    </header>
    <div id="user-profile">
        <div class="content-box-md">
            <div class="container02">
                <div class="row">
                    <div class="col-md-12 ">

                        <h2>Contact Us</h2>
                    </div>
                </div>
            </div>
        </div>


    </div>

    <!--contct us-->
    <section id="contactus">
        <div class="container-CU">
            @using (Html.BeginForm("contactus", "Home", FormMethod.Post, new { }))
            {
        <h2> Get in Touch</h2>
            <p>Let us know how to get back to you</p>

            <div class="row">
                <div class="col-md-6">
                    <label for="exampleInputPassword1" class="cu"><span>Full Name<span class="required">*</span></span></label>
                    @Html.TextBoxFor(x => x.FullName, new { @class = "form-control cu1", @PlaceHolder = "Enter your full name" })
                    @Html.ValidationMessageFor(model => model.FullName, "", new { @class = "text-danger erroemessage error_field" })

                    @*<input type="text" class="form-control cu1" placeholder=" Enter your full name" required>*@

                <div>
                    <label for="exampleInputEmail1" class="cu"><span>Email Address<span class="required">*</span></span></label>
                    @Html.TextBoxFor(x => x.EmailAddress, new { @class = "form-control cu1", @id = "cu-email", @PlaceHolder = "Enter your email address" })
                    @Html.ValidationMessageFor(model => model.EmailAddress, "", new { @class = "text-danger erroemessage error_field" })

                    @*<input type="email" class="form-control cu1" id="cu-email" aria-describedby="emailHelp" placeholder="Enter your email address">*@
                </div>
                <div>
                    <label for="exampleInputPassword1" class="cu"><span>Subject<span class="required">*</span></span></label>
                    @Html.TextBoxFor(x => x.Subject, new { @class = "form-control cu1", @PlaceHolder = "Enter your subject" })
                    @Html.ValidationMessageFor(model => model.Subject, "", new { @class = "text-danger erroemessage error_field" })
                    @*<input type="text" class="form-control cu1" placeholder=" Enter your subject" required>*@

                </div>
                </div>
                <div class="col-md-6">
                    <label for="exampleInputPassword1" class="cu3"><span>Comments/Questions</span><span class="required">*</span></label>
                    @Html.TextBoxFor(x => x.Comments, new { @class = "form-control cu2",  @PlaceHolder = "Comments..." })
                    @Html.ValidationMessageFor(model => model.Comments, "", new { @class = "text-danger erroemessage error_field" })
                    @*<input type="text" class="form-control cu2" placeholder=" Comments..." required>*@

                </div>

            </div>
            <div class="row">
                <div class="col-md-12">
                    <button class="btn btn-primary" type="submit" id="btn-s">SUBMIT</button>

                </div>
            </div>
          
            }
        </div>
    </section>
    <hr>
    <!--Footer Area-->
    <footer class="text-left">
        <div class="container010">
            <div class="row">
                <div class="col-md-6">
                    <p>
                        Copyright &copy; Tatvasoft All Rights Reserved.
                    </p>
                </div>

                <div class="col-md-6">
                    <ul class="social-list">
                        <li><a href="#"><i class="fa fa-facebook "></i></a></li>
                        <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                        <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>

    <script src="~/Scripts/js/jquery.min.js.js"></script>
    @*<script src="js/jquery.min.js.js"></script>*@
    <script src="~/Scripts/js/responsive-tabs/jquery.responsiveTabs.min.js"></script>
    @* <script src="js/responsive-tabs/jquery.responsiveTabs.min.js"></script>*@
    <!-- Bootstarp JS-->
    <script src="~/Scripts/js/bootstrap/bootstrap.min.js"></script>
    @* <script src="js/bootstrap/bootstrap.min.js"></script>*@
    <!--Custom JS-->
    <script src="~/Scripts/js/script.js"></script>
    @*<script src="js/script.js"></script>*@

</body>
</html>