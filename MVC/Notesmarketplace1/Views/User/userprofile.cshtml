    </header>
    <div id="user-profile">
        <div class="content-box-md">
            <div class="container02">
                <div class="row">
                    <div class="col-md-12 ">

                        <h2>User Profile</h2>
                    </div>
                </div>
            </div>
        </div>


    </div>
    <!--basic-->
    <section id="basic-profile">
        <div class="container-01 form-group">
            <div class="row">
                <!--left side-->
                <h3>Basic Profile Details</h3>
                <div class="col-md-6">

                    <div id="basic_left" class="basic-left center">
                        <div class="row">
                            <div class="col-md-12">

                                <label for="exampleInputPassword1" id="fn"><span>First Name<span class="required">*</span></span></label>
                                <input type="text" class="form-control" id="fname" placeholder=" Enter your first name" required>


                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">

                                <label for="exampleInputPassword1" id="email1">Email<span class="required">*</span></label>
                                <input type="email" class="form-control" id="email0" aria-describedby="emailHelp" placeholder=" Enter your email address" required>

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">

                                <label for="gender" id="gen">Gender</label><br>
                                <select id="Gender" class="form-control">
                                    <option value="" disabled selected hidden>Select your gender</option>
                                    <option value="male">Male</option>
                                    <option value="female">Female</option>
                                    <option value="other">other</option>
                                </select>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">

                                <label id="pp">Profile Picture</label>
                                <input type="picture" class="form-control" id="prifile_p" placeholder=" Upload a picture">

                            </div>
                        </div>

                    </div>
                </div>
                <!--right side-->

                <div class="col-md-6">
                    <div id="basic_right" class="basic-right up01">

                        <div class="row">
                            <div class="col-md-12">

                                <label for="exampleInputPassword1" id="ln">Last Name<span class="required">*</span></label>
                                <input type="text" class="form-control" id="lname" placeholder=" Enter your last name" required>


                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">


                                <label for="dob" id="DOB">Date Of Birth</label>
                                <input type="date" class="form-control" id="dob" placeholder="Enter your Date of Birth">
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-4">

                                <label for="phone" id="PHONE">Phone Number</label>
                                <select id="phone" class="form-control">
                                    <option value="" disabled selected hidden>+91</option>
                                </select>

                            </div>
                            <div class="col-md-8">

                                <input type="tel" class="form-control" id="Phone" placeholder="Enter your phone number">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--address-->
    <section id="Address">
        <div class="container-03 form-group">
            <div class="row">
                <!--left side-->
                <h3>Address Details</h3>
                <div class="col-md-6">

                    <div id="basic_left" class="basic-left center">
                        <div class="row">
                            <div class="col-md-12">

                                <label for="exampleInputPassword1" id="fn"><span>Adress Line 1<span class="required">*</span></span></label>
                                <input type="text" class="form-control" id="fname" placeholder=" Enter your adrees" required>


                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">

                                <label for="exampleInputPassword1" id="email1">City<span class="required">*</span></label>
                                <input type="text" class="form-control" id="email0" placeholder=" Enter your city" required>

                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">

                                <label for="exampleInputPassword1" id="email1">Zip<span class="required">*</span></label>
                                <input type="text" class="form-control" id="email0" placeholder=" Enter your zipcode" required>
                            </div>
                        </div>


                    </div>
                </div>
                <!--right side-->

                <div class="col-md-6">
                    <div id="basic_right" class="basic-right">

                        <div class="row">
                            <div class="col-md-12">

                                <label for="exampleInputPassword1" id="fn">Adress Line 2</label>
                                <input type="text" class="form-control" id="fname" placeholder=" Enter your address">


                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">

                                <label for="exampleInputPassword1" id="email1">State<span class="required">*</span></label>
                                <input type="text" class="form-control" id="email0" placeholder=" Enter your State" required>


                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <label for="gender" id="gen">Country</label><br>
                                <select id="Gender" class="form-control">
                                    <option value="" disabled selected hidden>Select your country</option>

                                </select>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </section>

    <!---university nd college-->
    <section id="U-C">
        <div class="container-04 form-group">
            <div class="row">
                <!--left side-->
                <h3>University and College Information</h3>
                <div class="col-md-6">

                    <div id="basic_left" class="basic-left center">
                        <div class="row">
                            <div class="col-md-12">

                                <label for="exampleInputPassword1" id="fn">University</label>
                                <input type="text" class="form-control" id="fname" placeholder=" Enter your university" required>


                            </div>
                        </div>
                        <br>



                    </div>
                </div>
                <!--right side-->

                <div class="col-md-6">
                    <div id="basic_right" class="basic-right">

                        <div class="row">
                            <div class="col-md-12">

                                <label for="exampleInputPassword1" id="fn">College</label>
                                <input type="text" class="form-control" id="fname" placeholder=" Enter your college">


                            </div>
                        </div>

                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <button class="btn btn-primary" type="submit" id="btn-sub">SUBMIT</button>

                    </div>
                </div>
            </div>
        </div>

    </section>

    <hr>
    <!--Footer Area-->
    <footer class="text-left">
        <div class="container010">
            <div class="row">
                <div class="col-md-6">
                    <p>
                        Copyright &copy; Tatvasoft All Rights Reserved.
                    </p>
                </div>

